{
  "$schema": "https://json.schemastore.org/tsconfig",
  "_version": "24.0.0",

  "compilerOptions": {
    "lib": ["esnext"],
    "module": "nodenext",
    "target": "esnext",

    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "moduleResolution": "node16",
    "allowImportingTsExtensions": true,
    "noEmit": true
  }
}